{
  "agentId": "iterator",
  "version": "2.0.0",
  "c1": "steps",
  "pathTemplate": "{c1}/{c2}/{c3}/f_{edition}_{adaptation}.md",
  "pathTemplateNoAdaptation": "{c1}/{c2}/{c3}/f_{edition}.md",
  "userPromptsBase": ".agent/iterator/prompts",
  "entryStepMapping": {
    "issue": "initial.issue",
    "iterate": "initial.iterate",
    "project": "initial.project.preparation"
  },
  "steps": {
    "initial.issue": {
      "stepId": "initial.issue",
      "name": "Issue Initial Prompt",
      "description": "Initial prompt when working on a single GitHub issue",
      "c2": "initial",
      "c3": "issue",
      "edition": "default",
      "fallbackKey": "initial.issue",
      "uvVariables": ["issue_number"],
      "usesStdin": false
    },
    "continuation.issue": {
      "stepId": "continuation.issue",
      "name": "Issue Continuation Prompt",
      "description": "Continuation prompt for issue iterations",
      "c2": "continuation",
      "c3": "issue",
      "edition": "default",
      "fallbackKey": "continuation.issue",
      "uvVariables": ["issue_number", "completed_iterations"],
      "usesStdin": false
    },
    "initial.iterate": {
      "stepId": "initial.iterate",
      "name": "Iterate Initial Prompt",
      "description": "Initial prompt for iteration-based execution",
      "c2": "initial",
      "c3": "iterate",
      "edition": "default",
      "fallbackKey": "initial.iterate",
      "uvVariables": ["iterations"],
      "usesStdin": true
    },
    "continuation.iterate": {
      "stepId": "continuation.iterate",
      "name": "Iterate Continuation Prompt",
      "description": "Continuation prompt for iteration-based execution",
      "c2": "continuation",
      "c3": "iterate",
      "edition": "default",
      "fallbackKey": "continuation.iterate",
      "uvVariables": ["completed_iterations", "remaining"],
      "usesStdin": false
    },
    "initial.project.preparation": {
      "stepId": "initial.project.preparation",
      "name": "Project Preparation Prompt",
      "description": "Preparation phase prompt for project mode",
      "c2": "initial",
      "c3": "project",
      "edition": "preparation",
      "fallbackKey": "initial.project.preparation",
      "uvVariables": [
        "project_number",
        "project_title",
        "label_info",
        "total_issues"
      ],
      "usesStdin": true
    },
    "initial.project.preparation.empty": {
      "stepId": "initial.project.preparation.empty",
      "name": "Project Preparation (No Issues)",
      "description": "Preparation phase prompt when no issues found",
      "c2": "initial",
      "c3": "project",
      "edition": "preparation",
      "adaptation": "empty",
      "fallbackKey": "initial.project.preparation.empty",
      "uvVariables": [
        "project_number",
        "project_title",
        "label_info",
        "label_filter"
      ],
      "usesStdin": false
    },
    "initial.project.processing.empty": {
      "stepId": "initial.project.processing.empty",
      "name": "Project Processing (No Current Issue)",
      "description": "Processing phase prompt when no current issue",
      "c2": "initial",
      "c3": "project",
      "edition": "processing",
      "adaptation": "empty",
      "fallbackKey": "initial.project.processing.empty",
      "uvVariables": [
        "project_number",
        "label_info",
        "project_title",
        "label_filter"
      ],
      "usesStdin": false
    },
    "initial.project.review": {
      "stepId": "initial.project.review",
      "name": "Project Review Prompt",
      "description": "Review phase prompt for project mode",
      "c2": "initial",
      "c3": "project",
      "edition": "review",
      "fallbackKey": "initial.project.review",
      "uvVariables": [
        "project_number",
        "project_title",
        "label_info",
        "issues_completed",
        "label_filter"
      ],
      "usesStdin": true
    },
    "initial.project.again": {
      "stepId": "initial.project.again",
      "name": "Project Again Prompt",
      "description": "Re-execution phase prompt after failed review",
      "c2": "initial",
      "c3": "project",
      "edition": "again",
      "fallbackKey": "initial.project.again",
      "uvVariables": ["project_number", "project_title", "label_info"],
      "usesStdin": false
    },
    "initial.project.complete": {
      "stepId": "initial.project.complete",
      "name": "Project Complete Message",
      "description": "Completion message for project mode",
      "c2": "initial",
      "c3": "project",
      "edition": "complete",
      "fallbackKey": "initial.project.complete",
      "uvVariables": ["project_number", "label_info", "issues_completed"],
      "usesStdin": false
    },
    "continuation.project.preparation": {
      "stepId": "continuation.project.preparation",
      "name": "Project Preparation Continuation",
      "description": "Continuation prompt for preparation phase",
      "c2": "continuation",
      "c3": "project",
      "edition": "preparation",
      "fallbackKey": "continuation.project.preparation",
      "uvVariables": ["completed_iterations"],
      "usesStdin": false
    },
    "continuation.project.processing.done": {
      "stepId": "continuation.project.processing.done",
      "name": "Project Processing Done",
      "description": "Message when all issues are processed",
      "c2": "continuation",
      "c3": "project",
      "edition": "processing",
      "adaptation": "done",
      "fallbackKey": "continuation.project.processing.done",
      "uvVariables": [
        "project_number",
        "completed_iterations",
        "issues_completed"
      ],
      "usesStdin": false
    },
    "continuation.project.review": {
      "stepId": "continuation.project.review",
      "name": "Project Review Continuation",
      "description": "Continuation prompt for review phase",
      "c2": "continuation",
      "c3": "project",
      "edition": "review",
      "fallbackKey": "continuation.project.review",
      "uvVariables": ["completed_iterations"],
      "usesStdin": false
    },
    "continuation.project.again": {
      "stepId": "continuation.project.again",
      "name": "Project Again Continuation",
      "description": "Continuation prompt for again phase",
      "c2": "continuation",
      "c3": "project",
      "edition": "again",
      "fallbackKey": "continuation.project.again",
      "uvVariables": ["completed_iterations"],
      "usesStdin": false
    },
    "continuation.project.complete": {
      "stepId": "continuation.project.complete",
      "name": "Project Complete Continuation",
      "description": "Continuation message for complete phase",
      "c2": "continuation",
      "c3": "project",
      "edition": "complete",
      "fallbackKey": "continuation.project.complete",
      "uvVariables": ["project_number"],
      "usesStdin": false
    },
    "section.project.context": {
      "stepId": "section.project.context",
      "name": "Project Context Section",
      "description": "Project context section inserted into issue prompts when part of a project",
      "c2": "section",
      "c3": "project",
      "edition": "context",
      "fallbackKey": "section.project.context",
      "uvVariables": [
        "project_number",
        "project_title",
        "label_info",
        "current_index",
        "total_issues"
      ],
      "usesStdin": false
    }
  }
}
